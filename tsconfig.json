{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"module": "esnext",
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
				"@napkin-ide/lcu-data-apps-common": [
					"dist/common"
				],
				"@napkin-ide/lcu-data-apps-common/*": [
					"dist/common/*"
				]
			},
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true,
		"enableIvy": false,
		"strictTemplates": true
	}
}